<div>
    <label for="your-title">What is your title?</label>
    @ErrorTextHelper.Error("TitleCode")
    @HtmlCustomHelpers.DisplayReferenceDataDropdown(@ViewBag.TitleTypes, "proposer-title", "value: TitleCode", "Please select...", true)
</div>
<div>
<label for="firstName">First Name:</label>
<input type="text" data-bind="value: FirstName"/>
</div>
<div>
<label for="lastName">Last Name:</label>
<input type="text" data-bind="value: LastName"/>
</div>
<div>
<label for="maritalStatus">Marital Status:</label>
@HtmlCustomHelpers.DisplayReferenceDataDropdown(@ViewBag.MaritalStatusTypes, "marital-status", "value: MaritalStatus", "Please select...", true)
</div>
<div>
<label for="address">Address:</label>
<input type="text" />
</div>
<div>
<label for="fullName">Full Name:</label>
<input type="text" data-bind="value: FullName"/>
</div>
<div>
<button name="next" id="personal-details-next" data-bind="click: $parent.FinishSection">Save</button>
</div>
