</div>
<div>
<label for="firstName">First Name:</label>
<input type="text" />
</div>
<div>
<label for="lastName">Last Name:</label>
<input type="text" />
</div>
<div>
<label for="maritalStatus">Marital Status:</label>
@HtmlCustomHelpers.DisplayReferenceDataDropdown(@ViewBag.MaritalStatusTypes, "marital-status", "value: MaritaStatus", "Please select...", true)
</div>
<div>
<label for="address">Address:</label>
<input type="text" />
</div>
